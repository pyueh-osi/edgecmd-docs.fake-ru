{
  "metadata": [
    {
      "src": [
        {
          "files": ["src/**.csproj"]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "_appLogoPath": "V1.1/images/atlas_icon.png",
      "_appName": "OSIsoft Edge System"
    }
  ],
  "build": {
    "globalMetadata": {
      "_enableSearch": true
    },
    "content": [
      {
        "files": [
          "V*/**/toc.yml",
          "V*/**/**.md",
          "V*/toc.yml",
          "toc.yml",
          "**.md"
        ]
      }
    ],
    "resource": [
      {
        "files": ["V1.1/images/**"]
      }
    ],
    "overwrite": [
      {
        "files": ["apidoc/**.md"],
        "exclude": ["obj/**", "_site/**"]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "_exported_templates/default"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
